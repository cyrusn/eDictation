{
  "name": "e_dictation_server",
  "version": "1.0.0",
  "description": "An application for dictation",
  "main": "app.js",
  "scripts": {
    "start": "NODE_ENV=production node ./app.js",
    "dev": "NODE_ENV=development nodemon ./server/server.js",
    "db": "mongod --dbpath=./database",
    "test": "mocha",
    "build": "NODE_ENV=production browserify -t [ babelify ] ./client/index.js -o ./public/bundle.js",
    "watch": "watchify ./client/index.js -o ./public/bundle.js -dv -t [ babelify ]"
  },
  "keywords": [
    "dictation"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cyrusn/eDictation.git"
  },
  "author": [
    "CyrusN",
    "SamF"
  ],
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^1.0.2",
    "boom": "^4.3.0",
    "colors": "^1.1.2",
    "good": "^7.1.0",
    "good-console": "^6.4.0",
    "good-file": "^6.0.1",
    "good-squeeze": "^5.0.1",
    "hapi": "^16.1.0",
    "hapi-auth-jwt2": "^7.2.4",
    "hapi-swagger": "^7.7.0",
    "inert": "^4.1.0",
    "joi": "^10.2.2",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "mongoose": "^4.9.1",
    "tracer": "^0.8.7",
    "vision": "^4.1.1",
    "yargs": "^7.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.2",
    "babel-preset-env": "^1.3.3",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "eslint": "^3.19.0",
    "freezer-js": "^0.11.2",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "watchify": "^3.9.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "globals": [
      "fetch"
    ]
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  }
}
